// Place your key bindings in this file to override the defaults
[
  {
    "key": "ctrl+;",
    "command": "workbench.action.terminal.focus",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+;",
    "command": "workbench.action.focusFirstEditorGroup",
    "when": "!editorTextFocus"
  },
  {
    "key": "cmd+[",
    "command": "editor.action.jumpToBracket",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+]",
    "command": "editor.action.smartSelect.grow",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+[",
    "command": "editor.emmet.action.matchTag",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+3",
    "command": "workbench.action.navigateBack",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+4",
    "command": "workbench.action.navigateForward",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+n",
    "command": "workbench.action.files.newUntitledFile",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+n",
    "command": "-workbench.action.files.newUntitledFile"
  },
  {
    "key": "cmd+n",
    "command": "workbench.action.terminal.new",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+pageup",
    "command": "workbench.action.terminal.focusNext",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+pagedown",
    "command": "workbench.action.terminal.focusPrevious",
    "when": "terminalFocus"
  },
  {
    "key": "cmd+shift+-",
    "command": "workbench.action.zoomIn"
  },
  {
    "key": "f8",
    "command": "workbench.action.editor.nextChange",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+f8",
    "command": "workbench.action.editor.previousChange",
    "when": "editorTextFocus"
  },
  {
    "key": "f8",
    "command": "workbench.action.compareEditor.nextChange",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+f8",
    "command": "workbench.action.compareEditor.previousChange",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+j",
    "command": "-workbench.action.togglePanel"
  },
  {
    "key": "cmd+shift+w",
    "command": "-workbench.action.closeWindow"
  },
  {
    "key": "cmd+f14",
    "command": "copyRelativeFilePath",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+shift+f14",
    "command": "copy-file-name.copyFileName",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+l",
    "command": "eslint.executeAutofix",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+shift+l",
    "command": "editor.action.formatDocument",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+shift+l",
    "command": "-editor.action.selectHighlights",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+shift+d",
    "command": "editor.action.selectHighlights",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+f13",
    "command": "editor.action.showContextMenu"
  },
  {
    "key": "cmd+shift+f13",
    "command": "editor.action.triggerSuggest",
    "when": "editorHasCompletionItemProvider && textInputFocus && !editorReadonly"
  },
  {
    "key": "cmd+k",
    "command": "-aipopup.action.modal.generate",
    "when": "editorFocus"
  },
  {
    "key": "cmd+alt+k",
    "command": "aipopup.action.modal.generate",
    "when": "editorFocus"
  },
  {
    "key": "cmd+l",
    "command": "-aichat.newchataction"
  },
  {
    "key": "alt+cmd+l",
    "command": "aichat.newchataction"
  },
  {
    "key": "cmd+k cmd+l",
    "command": "editor.toggleFold",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
    "key": "cmd+i",
    "command": "-toggleSuggestionDetails",
    "when": "suggestWidgetHasFocusedSuggestion && suggestWidgetVisible && textInputFocus"
  },
  {
    "key": "cmd+i",
    "command": "-emojisense.quickEmoji",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+i",
    "command": "-emojisense.quickEmojiTerminal",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+alt+i",
    "command": "emojisense.quickEmojitext",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+cmd+i",
    "command": "-emojisense.quickEmojitext",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+alt+i",
    "command": "emojisense.quickEmojitextTerminal",
    "when": "terminalFocus"
  },
  {
    "key": "shift+cmd+i",
    "command": "-emojisense.quickEmojitextTerminal",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+cmd+;",
    "command": "workbench.action.closePanel"
  },
  {
    "key": "alt+cmd+f",
    "command": "-copy-file-name.copyFileName",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+cmd+e",
    "command": "-copy-file-name.copyFileNameWithExtension",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+d cmd+b",
    "command": "-dbcode.connections.view.focus",
    "when": "editorHasMultipleSelections && !editorTextFocus && !terminalFocusInAny || editorHasSelection && !editorTextFocus && !terminalFocusInAny || !editorHasMultipleSelections && !editorHasSelection && !terminalFocusInAny"
  },
  {
    "key": "cmd+delete",
    "command": "deleteFile",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceMoveableToTrash && !inputFocus"
  },
  {
    "key": "cmd+backspace",
    "command": "-deleteFile",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceMoveableToTrash && !inputFocus"
},
{
    "key": "shift+enter",
    "command": "workbench.action.terminal.sendSequence",
    "args": {
        "text": "\u001b\r"
    },
    "when": "terminalFocus"
}
]
